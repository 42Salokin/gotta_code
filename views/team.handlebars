<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Create Team</title>
  <link rel="stylesheet" href="../public/assets/style.css">
</head>
<body>
  <!-- Create Team Form -->
  <form id="create-team-form">
  <label for="team-name">Team Name:</label>
  <input type="text" id="team-name" name="team-name" required>

  <label for="pokemon1">Pokémon 1:</label>
  <input type="text" id="pokemon1" name="pokemon1">

  <label for="pokemon2">Pokémon 2:</label>
  <input type="text" id="pokemon2" name="pokemon2">

  <label for="pokemon3">Pokémon 3:</label>
  <input type="text" id="pokemon3" name="pokemon3">

  <label for="pokemon4">Pokémon 4:</label>
  <input type="text" id="pokemon4" name="pokemon4">

  <label for="pokemon5">Pokémon 5:</label>
  <input type="text" id="pokemon5" name="pokemon5">

  <label for="pokemon6">Pokémon 6:</label>
  <input type="text" id="pokemon6" name="pokemon6">

  <button type="submit">Create Team</button>
</form>

<script src="/js/team.js"></script>
</body>