<!-- This is the header section with the developer's name and navigation links -->
<h1>{{gotta code them all}}</h1>
<nav>
    <ul>
        <li><a href="/home">Home</a></li>
        <li><a href="/login">login</a></li>
        <li><a href="/signup">signup</a></li>
        <li><a href="/logout">logout</a></li>
        <li><a href="/pokedex">pokedex</a></li>
        <li><a href="/teams">teams</a></li>
        <li><a href="/evolutions">evolutions</a></li>
        <li><a href="/credits">credits</a></li>
    </ul>
</nav>
<!-- This is the main content section with the developer's bio and introductory text -->
<div>
    <h2></h2>
    <p>{{developerBio}}</p>
</div>
<!-- This is the portfolio section with a project card displaying project information -->
<div>
    <h2>Gotta code em all</h2>
    {{#each pokemon}}
        <div class="pokemon-card">
            <img src="{{projectImage}}" alt="{{projectTitle}}">
            <h3>{{projectTitle}}</h3>
            <p>{{projectDescription}}</p>
            <a href="{{projectLink}}">View Project</a>
            <a href="{{githubLink}}">GitHub Repository</a>
        </div>
    {{/each}}
</div>
<!-- This is the contact section with a simple contact form -->
<div>
    <h2>signup</h2>
    <form>
        <label for="username">username:</label>
        <input type="text" id="username" name="name">
        <label for="email">Email:</label>
        <input type="email" id="email" name="email">
        <button type="submit">create</button>
    </form>
</div>
<!-- This is the resume section with a link to download the developer's resume -->
<div>
    <h2>Pick a pokemon to add to your team</h2>
    <a href="{{resumeLink}}">Download Resume</a>
    <ul>
        {{#each skills}}
            <li>{{this}}</li>
        {{/each}}
    </ul>
</div>
<!-- This is the footer section with links to the developer's profiles -->
<footer>
    <p>We hope to see you again!</p>
    <a href="{{githubProfile}}">GitHub</a>
    
</footer>